<rdf:RDF
        xmlns="http://www.broadinstitute.org/ontologies/DUOS#"
        xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
        xmlns:owl="http://www.w3.org/2002/07/owl#"
        xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
        xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
        xmlns:oboInOwl="http://www.geneontology.org/formats/oboInOwl#"
        xml:base="http://www.broadinstitute.org/ontologies/DUOS">

    <owl:Ontology rdf:about="http://www.broadinstitute.org/ontologies/DUOS">
        <rdfs:label rdf:datatype="&xsd;string">Broad Data Use Oversight System Ontology</rdfs:label>
    </owl:Ontology>

    <!-- 
         ///////////////////////////////////////////////////////////////////////////////////////
         //
         // Object Properties
         //
         ///////////////////////////////////////////////////////////////////////////////////////
         -->

    <!-- http://www.broadinstitute.org/ontologies/DUOS/bornIn -->
    <owl:ObjectProperty rdf:about="http://www.broadinstitute.org/ontologies/DUOS/bornIn"/>

    <!-- http://www.broadinstitute.org/ontologies/DUOS/restrictedTo -->
    <owl:ObjectProperty rdf:about="http://www.broadinstitute.org/ontologies/DUOS/restrictedTo">
        <rdfs:label>restrictedTo</rdfs:label>
    </owl:ObjectProperty>

    <!--
         ///////////////////////////////////////////////////////////////////////////////////////
         //
         // Classes
         //
         ///////////////////////////////////////////////////////////////////////////////////////
         -->

    <!-- http://www.broadinstitute.org/ontologies/DUOS/research_type -->
    <owl:Class rdf:about="http://www.broadinstitute.org/ontologies/DUOS/research_type"/>

    <!-- http://www.broadinstitute.org/ontologies/DUOS/methods_research -->
    <owl:Class rdf:about="http://www.broadinstitute.org/ontologies/DUOS/methods_research">
        <rdfs:label rdf:datatype="&xsd;string">Methods Research</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.broadinstitute.org/ontologies/DUOS/research_type"/>
    </owl:Class>

    <!-- http://www.broadinstitute.org/ontologies/DUOS/aggregate_research -->
    <owl:Class rdf:about="http://www.broadinstitute.org/ontologies/DUOS/aggregate_research">
        <rdfs:label rdf:datatype="&xsd;string">Aggregate Research</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.broadinstitute.org/ontologies/DUOS/research_type"/>
    </owl:Class>

    <!-- http://www.broadinstitute.org/ontologies/DUOS/Africa -->
    <owl:Class rdf:about="http://www.broadinstitute.org/ontologies/DUOS/Africa">
        <rdfs:label rdf:datatype="&xsd;string">Africa</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.broadinstitute.org/ontologies/DUOS/geography"/>
    </owl:Class>

    <!-- http://www.broadinstitute.org/ontologies/DUOS/African_American_population -->
    <owl:Class rdf:about="http://www.broadinstitute.org/ontologies/DUOS/African_American_population">
        <rdfs:label rdf:datatype="&xsd;string">African-American population</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.broadinstitute.org/ontologies/DUOS/ethnicity"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.broadinstitute.org/ontologies/DUOS/bornIn"/>
                <owl:allValuesFrom rdf:resource="http://www.broadinstitute.org/ontologies/DUOS/United_States"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>

    <!-- http://www.broadinstitute.org/ontologies/DUOS/Asia -->
    <owl:Class rdf:about="http://www.broadinstitute.org/ontologies/DUOS/Asia">
        <rdfs:label rdf:datatype="&xsd;string">Asia</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.broadinstitute.org/ontologies/DUOS/geography"/>
    </owl:Class>

    <!-- http://www.broadinstitute.org/ontologies/DUOS/Broad -->
    <owl:Class rdf:about="http://www.broadinstitute.org/ontologies/DUOS/Broad">
        <rdfs:label rdf:datatype="&xsd;string">The Broad Institute</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.broadinstitute.org/ontologies/DUOS/Massachusetts"/>
    </owl:Class>

    <!-- http://www.broadinstitute.org/ontologies/DUOS/Cardiff -->
    <owl:Class rdf:about="http://www.broadinstitute.org/ontologies/DUOS/Cardiff">
        <rdfs:label rdf:datatype="&xsd;string">Cardiff</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.broadinstitute.org/ontologies/DUOS/Wales"/>
    </owl:Class>

    <!-- http://www.broadinstitute.org/ontologies/DUOS/Commercial_Status -->
    <owl:Class rdf:about="http://www.broadinstitute.org/ontologies/DUOS/Commercial_Status">
        <rdfs:label rdf:datatype="&xsd;string">Commercial Status</rdfs:label>
    </owl:Class>

    <!-- http://www.broadinstitute.org/ontologies/DUOS/East_Asian_population -->
    <owl:Class rdf:about="http://www.broadinstitute.org/ontologies/DUOS/East_Asian_population">
        <rdfs:label rdf:datatype="&xsd;string">East Asian population</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.broadinstitute.org/ontologies/DUOS/ethnicity"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.broadinstitute.org/ontologies/DUOS/bornIn"/>
                <owl:allValuesFrom rdf:resource="http://www.broadinstitute.org/ontologies/DUOS/Asia"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>

    <!-- http://www.broadinstitute.org/ontologies/DUOS/Europe -->
    <owl:Class rdf:about="http://www.broadinstitute.org/ontologies/DUOS/Europe">
        <rdfs:label rdf:datatype="&xsd;string">Europe</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.broadinstitute.org/ontologies/DUOS/geography"/>
    </owl:Class>

    <!-- http://www.broadinstitute.org/ontologies/DUOS/For_profit -->
    <owl:Class rdf:about="http://www.broadinstitute.org/ontologies/DUOS/For_profit">
        <rdfs:label rdf:datatype="&xsd;string">For profit</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.broadinstitute.org/ontologies/DUOS/Commercial_Status"/>
        <owl:disjointWith rdf:resource="http://www.broadinstitute.org/ontologies/DUOS/Non_profit"/>
    </owl:Class>

    <!-- http://www.broadinstitute.org/ontologies/DUOS/Latin_American_population -->
    <owl:Class rdf:about="http://www.broadinstitute.org/ontologies/DUOS/Latin_American_population">
        <rdfs:label rdf:datatype="&xsd;string">Latin-American population</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.broadinstitute.org/ontologies/DUOS/ethnicity"/>
    </owl:Class>

    <!-- http://www.broadinstitute.org/ontologies/DUOS/MGH -->
    <owl:Class rdf:about="http://www.broadinstitute.org/ontologies/DUOS/MGH">
        <rdfs:label rdf:datatype="&xsd;string">Massachusetts General Hospital</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.broadinstitute.org/ontologies/DUOS/Massachusetts"/>
    </owl:Class>

    <!-- http://www.broadinstitute.org/ontologies/DUOS/Massachusetts -->
    <owl:Class rdf:about="http://www.broadinstitute.org/ontologies/DUOS/Massachusetts">
        <rdfs:label rdf:datatype="&xsd;string">Massachusetts</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.broadinstitute.org/ontologies/DUOS/United_States"/>
    </owl:Class>

    <!-- http://www.broadinstitute.org/ontologies/DUOS/Non_profit -->
    <owl:Class rdf:about="http://www.broadinstitute.org/ontologies/DUOS/Non_profit">
        <rdfs:label rdf:datatype="&xsd;string">Nonprofit</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.broadinstitute.org/ontologies/DUOS/Commercial_Status"/>
    </owl:Class>

    <!-- http://www.broadinstitute.org/ontologies/DUOS/North_America -->
    <owl:Class rdf:about="http://www.broadinstitute.org/ontologies/DUOS/North_America">
        <rdfs:label rdf:datatype="&xsd;string">North America</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.broadinstitute.org/ontologies/DUOS/geography"/>
    </owl:Class>

    <!-- http://www.broadinstitute.org/ontologies/DUOS/South_America -->
    <owl:Class rdf:about="http://www.broadinstitute.org/ontologies/DUOS/South_America">
        <rdfs:label rdf:datatype="&xsd;string">South America</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.broadinstitute.org/ontologies/DUOS/geography"/>
    </owl:Class>

    <!-- http://www.broadinstitute.org/ontologies/DUOS/UK -->
    <owl:Class rdf:about="http://www.broadinstitute.org/ontologies/DUOS/UK">
        <rdfs:label rdf:datatype="&xsd;string">United Kingdom</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.broadinstitute.org/ontologies/DUOS/Europe"/>
    </owl:Class>

    <!-- http://www.broadinstitute.org/ontologies/DUOS/United_States -->
    <owl:Class rdf:about="http://www.broadinstitute.org/ontologies/DUOS/United_States">
        <rdfs:label rdf:datatype="&xsd;string">United States</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.broadinstitute.org/ontologies/DUOS/North_America"/>
    </owl:Class>

    <!-- http://www.broadinstitute.org/ontologies/DUOS/University_Cardiff -->
    <owl:Class rdf:about="http://www.broadinstitute.org/ontologies/DUOS/University_Cardiff">
        <rdfs:label rdf:datatype="&xsd;string">University of Cardiff</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.broadinstitute.org/ontologies/DUOS/Cardiff"/>
    </owl:Class>

    <!-- http://www.broadinstitute.org/ontologies/DUOS/Wales -->
    <owl:Class rdf:about="http://www.broadinstitute.org/ontologies/DUOS/Wales">
        <rdfs:label rdf:datatype="&xsd;string">Wales</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.broadinstitute.org/ontologies/DUOS/UK"/>
    </owl:Class>

    <!-- http://www.broadinstitute.org/ontologies/DUOS/children -->
    <owl:Class rdf:about="http://www.broadinstitute.org/ontologies/DUOS/children">
        <rdfs:label rdf:datatype="&xsd;string">children</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.broadinstitute.org/ontologies/DUOS/population"/>
        <rdfs:comment rdf:datatype="&xsd;string">&apos;children&apos; here means &apos;children under the age of 18&apos;.</rdfs:comment>
    </owl:Class>

    <!-- http://www.broadinstitute.org/ontologies/DUOS/ethnicity -->
    <owl:Class rdf:about="http://www.broadinstitute.org/ontologies/DUOS/ethnicity">
        <rdfs:label rdf:datatype="&xsd;string">ethnicity</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.broadinstitute.org/ontologies/DUOS/population"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.broadinstitute.org/ontologies/DUOS/bornIn"/>
                <owl:allValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://www.broadinstitute.org/ontologies/DUOS/North_America"/>
                            <rdf:Description rdf:about="http://www.broadinstitute.org/ontologies/DUOS/South_America"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:allValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>

    <!-- http://www.broadinstitute.org/ontologies/DUOS/geography -->
    <owl:Class rdf:about="http://www.broadinstitute.org/ontologies/DUOS/geography">
        <rdfs:label rdf:datatype="&xsd;string">geography</rdfs:label>
    </owl:Class>

    <!-- http://www.broadinstitute.org/ontologies/DUOS/male -->
    <owl:Class rdf:about="http://www.broadinstitute.org/ontologies/DUOS/male">
        <rdfs:label rdf:datatype="&xsd;string">Male</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.broadinstitute.org/ontologies/DUOS/population"/>
        <owl:disjointWith rdf:resource="http://www.broadinstitute.org/ontologies/DUOS/female"/>
    </owl:Class>

    <!-- http://www.broadinstitute.org/ontologies/DUOS/boys -->
    <owl:Class rdf:about="http://www.broadinstitute.org/ontologies/DUOS/boys">
        <rdfs:comment rdf:datatype="&xsd;string">Males under the age of 18</rdfs:comment>
        <rdfs:label rdf:datatype="&xsd;string">boys</rdfs:label>
        <owl:intersectionOf rdf:parseType="Collection">
            <owl:Class rdf:about="#male"/>
            <owl:Class rdf:about="#children"/>
        </owl:intersectionOf>
        <rdfs:subClassOf rdf:resource="http://www.broadinstitute.org/ontologies/DUOS/male"/>
        <rdfs:subClassOf rdf:resource="http://www.broadinstitute.org/ontologies/DUOS/children"/>
    </owl:Class>

    <!-- http://www.broadinstitute.org/ontologies/DUOS/population -->
    <owl:Class rdf:about="http://www.broadinstitute.org/ontologies/DUOS/population"/>

    <!-- http://www.broadinstitute.org/ontologies/DUOS/female -->
    <owl:Class rdf:about="http://www.broadinstitute.org/ontologies/DUOS/female">
        <rdfs:label rdf:datatype="&xsd;string">Female</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.broadinstitute.org/ontologies/DUOS/population"/>
    </owl:Class>

    <!-- http://www.broadinstitute.org/ontologies/DUOS/girls -->
    <owl:Class rdf:about="http://www.broadinstitute.org/ontologies/DUOS/girls">
        <rdfs:comment rdf:datatype="&xsd;string">Females under the age of 18</rdfs:comment>
        <rdfs:label rdf:datatype="&xsd;string">Girls</rdfs:label>
        <owl:intersectionOf rdf:parseType="Collection">
            <owl:Class rdf:about="#female"/>
            <owl:Class rdf:about="#children"/>
        </owl:intersectionOf>
        <rdfs:subClassOf rdf:resource="http://www.broadinstitute.org/ontologies/DUOS/female"/>
        <rdfs:subClassOf rdf:resource="http://www.broadinstitute.org/ontologies/DUOS/children"/>
    </owl:Class>

    <!-- http://www.broadinstitute.org/ontologies/DUOS/dataset_usage -->
    <owl:Class rdf:about="http://www.broadinstitute.org/ontologies/DUOS/dataset_usage">
        <rdfs:comment rdf:datatype="&xsd;string">Dataset Usage</rdfs:comment>
        <rdfs:label rdf:datatype="&xsd;string">Dataset Usage</rdfs:label>
    </owl:Class>

    <!-- http://www.broadinstitute.org/ontologies/DUOS/control -->
    <owl:Class rdf:about="http://www.broadinstitute.org/ontologies/DUOS/control">
        <rdfs:comment rdf:datatype="&xsd;string">To be used as a Control Set</rdfs:comment>
        <rdfs:label rdf:datatype="&xsd;string">Control Set</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.broadinstitute.org/ontologies/DUOS/dataset_usage"/>
    </owl:Class>
    
    <!-- http://www.broadinstitute.org/ontologies/DUOS/population_structure -->
    <owl:Class rdf:about="http://www.broadinstitute.org/ontologies/DUOS/population_structure">
        <rdfs:comment rdf:datatype="&xsd;string">To be used in the study the genetic structure of a population or to characterize the normal variation in the population</rdfs:comment>
        <rdfs:label rdf:datatype="&xsd;string">Population Structure</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.broadinstitute.org/ontologies/DUOS/dataset_usage"/>
    </owl:Class>
    
    <!-- 
         ///////////////////////////////////////////////////////////////////////////////////////
         //
         // General axioms
         //
         ///////////////////////////////////////////////////////////////////////////////////////
         -->
    <rdf:Description>
        <rdf:type rdf:resource="&owl;AllDisjointClasses"/>
        <owl:members rdf:parseType="Collection">
            <rdf:Description rdf:about="http://www.broadinstitute.org/ontologies/DUOS/Africa"/>
            <rdf:Description rdf:about="http://www.broadinstitute.org/ontologies/DUOS/Asia"/>
            <rdf:Description rdf:about="http://www.broadinstitute.org/ontologies/DUOS/Europe"/>
            <rdf:Description rdf:about="http://www.broadinstitute.org/ontologies/DUOS/North_America"/>
            <rdf:Description rdf:about="http://www.broadinstitute.org/ontologies/DUOS/South_America"/>
        </owl:members>
    </rdf:Description>
</rdf:RDF>
